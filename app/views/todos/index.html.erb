<h1>Todos List</h1>
<%= link_to 'New Item', new_todo_path %>
<% @todos.each do |todo| %>
  <h2><%= todo.title %></h2>
  <a class="btn"<%= link_to 'Show Item', todo_path(todo) %>
  <a class="btn"<%= link_to 'Edit Item', edit_todo_path(todo) %>
  <a class="btn"<%= link_to 'Completed Task', todo, method: :delete %>
  <% end %>
